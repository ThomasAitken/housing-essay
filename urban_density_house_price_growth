digraph {
	dpi=300 nodesep=0.4 rankdir=TB ranksep=1.1
	edge [minlen=1]
	graph [fontsize=16]
	A [label="Initial Urban Density" color="#D97B00" fillcolor="#FFD580" fontsize=16 penwidth=2 shape=box style=filled]
	B [label="Differences in Long-Run House Price Growth" fillcolor="#D97B00" fontsize=16 penwidth=2 shape=box style=filled]
	B [label="Differences in Long-Run House Price Growth" color="#D97B00" fillcolor="#FFD580" penwidth=2 shape=box style=filled]
	A -> B [weight=5]
	L1 [label="" height=0 shape=point style=invis width=0]
	R1 [label="" height=0 shape=point style=invis width=0]
	subgraph row_A {
		rank=same
		L1
		A
		R1
	}
	L1 -> A [style=invis weight=10]
	A -> R1 [style=invis weight=10]
	BENDL [label="" height=0 shape=point style=invis width=0]
	subgraph row_B {
		rank=same
		BENDL
		B
	}
	BENDL -> B [style=invis weight=10]
	subgraph cluster_CT1 {
		color="#E6F3FF" fontcolor="#005B96" label="Causal Theory 1: Land Value Growth" style=filled
		C1 [label="City population increases" shape=ellipse]
		C2 [label="Land value grows even without
supply-demand mismatch" shape=ellipse]
		C3 [label="Low density means more properties with land"]
		C1 -> C2
		C2 -> C3
	}
	subgraph cluster_CT2 {
		color="#E6F3FF" fontcolor="#005B96" label="Causal Theory 2: Supply-Demand Mismatch" style=filled
		D1 [label="Low-density cities face building obstacles" shape=ellipse]
		D1a [label="Expensive prime land" shape=note]
		D1b [label="Political resistance" shape=note]
		D1c [label="Poor public transport" shape=note]
		D1d [label="Cultural preference for land" shape=note]
		D1 -> D1a
		D1 -> D1b
		D1 -> D1c
		D1 -> D1d
		D2 [label="Supply-demand mismatch" shape=ellipse]
		D3 [label="Mismatch drives short-term spikes" shape=ellipse]
		D1 -> D2
		D2 -> D3
	}
	A -> C1
	A -> D1
	C3 -> B [weight=6]
	D3 -> B [weight=6]
	L1 -> C1 [style=invis weight=10]
	R1 -> D1 [style=invis weight=10]
	subgraph cluster_CT3 {
		color="#E6F3FF" fontcolor="#005B96" label="Causal Theory 3: Positive Feedback" style=filled
		E1 [label="Price growth anchors expectations" shape=ellipse]
		E2 [label="Willingness to take larger mortgages" shape=ellipse]
		E3 [label="Reinforces further price growth" shape=ellipse]
		E1 -> E2
		E2 -> E3
		E3 -> E1 [constraint=false style=dashed]
	}
	B -> E1
	C3 -> BENDL [constraint=false style=dotted]
	BENDL -> E1 [constraint=false style=dotted]
	D3 -> E1 [constraint=false style=dotted]
}
